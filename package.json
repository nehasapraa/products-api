{
  "name": "products-api",
  "version": "1.0.0",
  "description": "Products api exercise",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "test": "jest --coverage",
    "build": "npm-run-all clean transpile",
    "server": "node ./build/bin/www",
    "dev": "pm2-dev start config/dev-config.json",
    "prod": "pm2-runtime start ecosystem.config.js --env production",
    "transpile": "babel ./server --out-dir build",
    "clean": "rimraf build",
    "watch:dev": "nodemon",
    "test:debug": "mocha --require @babel/register --inspect-brk --exit",
    "lint": "eslint -c .eslintrc.json {server,test}/**",
    "lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**"
  },
  "author": "Neha",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.9.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.1.1",
    "bookshelf-bulk-save": "^1.0.6",
    "bookshelf-column-cache": "^1.0.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "get-json": "^1.0.1",
    "jest": "^26.0.1",
    "mocha": "^7.0.1",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.4.0",
    "prettier": "^1.19.1",
    "qs": "^6.9.4",
    "query-string": "^6.11.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {},
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testURL": "http://dev-wooliesx-recruitment.azurewebsites.net"
  },
  "_moduleAliases": {
    "@app": "./server",
    "@controllers": "./server/api/v1/controllers",
    "@root": "."
  }
}
